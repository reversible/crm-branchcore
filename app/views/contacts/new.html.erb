<div class="row">
	<div class="span6 offset4">
		<% @page_title = 'Create contact' %>

		
			<h2>Create Admin User</h2>

			<%= form_for(:contact, :url => {:action => 'create'}) do |f| %>

			<%= render(:partial => 'form', :locals => {:f => f} ) %>
			<br />
			<div class="form-buttons" style="margin-left: 80px;">
				<%= link_to("<< Back to List", {:action => 'list'}, :class => 'btn btn-inverse') %>
				<%= submit_tag 'Create Contact', :class =>"btn btn-primary" %>
			</div>

			<% end %>
		

	</div>

</div>